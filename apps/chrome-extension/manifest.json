{
    "manifest_version": 3,
    "name": "Page-Stash",
    "version": "0.0.1",
    "description": "Bookmark your web pages with ease",
    "background": {
        "service_worker": "./src/background/index.ts",
        "type": "module"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "./src/content/index.ts"
            ]
        }
    ]
}